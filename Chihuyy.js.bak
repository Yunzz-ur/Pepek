// Fungsi untuk merestart bot menggunakan PM2
const restartBot = () => {
  pm2.connect((err) => {
    if (err) {
      console.error("Gagal terhubung ke PM2:", err);
      return;
    }

    pm2.restart("index", (err) => {
      // 'index' adalah nama proses PM2 Anda
      pm2.disconnect(); // Putuskan koneksi setelah restart
      if (err) {
        console.error("Gagal merestart bot:", err);
      } else {
        console.log("Bot berhasil direstart.");
      }
    });
  });
};

bot.command('info', async (ctx) => {
  const message = ctx.message;
  const entities = message.entities;

  let targetUser = null;

  // 1. Kalau pakai reply
  if (message.reply_to_message) {
    targetUser = message.reply_to_message.from;
  }

  // 2. Kalau pakai @mention dan user pernah kirim pesan
  else if (entities && entities.length > 0) {
    for (const entity of entities) {
      if (entity.type === 'text_mention') {
        targetUser = entity.user;
        break;
      } else if (entity.type === 'mention') {
        const username = message.text.slice(entity.offset + 1, entity.offset + entity.length);
        try {
          const userData = await ctx.telegram.getChat(`@${username}`);
          targetUser = userData;
          break;
        } catch (error) {
          return ctx.reply('âŒ Reply orangnya, bukan tag orang nya.');
        }
      }
    }
  }

  // 3. Kalau tidak ada target, pakai diri sendiri
  if (!targetUser) {
    targetUser = ctx.from;
  }

  const firstName = targetUser.first_name || '-';
  const userId = targetUser.id || '-';
  const username = targetUser.username ? `@${targetUser.username}` : '(Tidak ada)';
  const userLink = targetUser.username
    ? `tg://user?id=${userId}`
    : '(Tidak tersedia)';

  const result = `
<blockquote><b>CLICK ID TO COPY</b></blockquote>

<b>User info:</b>
<b>ID:</b> <code>${userId}</code>
<b>First Name:</b> ${firstName}
<b>Username:</b> ${username}
<b>User link:</b> <a href="tg://user?id=${userId}">Click here</a>`;

  await ctx.reply(result, { parse_mode: "HTML", disable_web_page_preview: true });
});

